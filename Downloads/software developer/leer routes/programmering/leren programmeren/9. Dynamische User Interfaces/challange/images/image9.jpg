<script>
//show/hide the dropdown country selector. 
window.addEventListener('click', function(e){
	if (document.getElementById('flow-country-picker-container').contains(e.target)) {
		$(".flow-country-picker-dropdown-trigger").addClass("flow-country-picker-open");
		$(".flow-country-picker-dropdown-menu").addClass("flow-country-picker-show");
	} else {
		$(".flow-country-picker-dropdown-trigger").removeClass("flow-country-picker-open");
		$(".flow-country-picker-dropdown-menu").removeClass("flow-country-picker-show");
	}
});

//declaration of variables according to the class/div value.
const elements = document.getElementsByClassName("flow-country-picker-dropdown-item");
const countryName = document.getElementsByClassName('flow-country-picker-dropdown-option-text');
const countryDefault = document.querySelector('.flow-country-picker-dropdown-trigger-text');
const countryCheckSvg = `
	<svg class="svg-check" style="align-self:center" width="16" height="16" viewBox="0 0 24 24" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">
	<defs>
		<path d="M8.9 21.583a1.78 1.78 0 0 1-1.259-.52l-7.12-7.12a1.78 1.78 0 1 1 2.518-2.518l5.757 5.757 12.07-14.157a1.781 1.781 0 0 1 2.709 2.31l-13.32 15.623a1.78 1.78 0 0 1-1.355.625z" id="a"></path>
	</defs>
	<use fill="#0051b2" xlink:href="#a" fill-rule="evenodd"></use>
</svg>
`;

//get the flow experience countries
const selCountryName = function () {	
	let selCountry = this.innerHTML;
	selCountry = selCountry.replace(/\s[()].*/, '');
	selCountry = selCountry.toLowerCase();
	selCountry = selCountry.replace(/ /g, '-');
	if (selCountry === "united-states") {
		location.href = 'https://www.timex.com/home';
	} else {
		location.href = 'https://www.timex.com/home' + '?flow_experience=' + selCountry;
	}
}

//get the undefined countries
const myCountrySelc = function() {
	let attribute = this.getAttribute("data-value");
	let countryRaw = attribute.match(/undefined/g);
	if( countryRaw ) {
		let countryDom =  attribute.replace('_undefined_','');
		if (countryDom === "CA") {
			location.href = 'https://www.timex.ca/home';
		} else if (countryDom === "PL") {
			location.href = 'https://www.timex.pl';
		} else if (countryDom === "GB") {
			location.href = 'https://www.timex.co.uk/home';
		} else if (countryDom === "EU" && location.host !== "www.timex.co.uk") {
			location.href = 'https://www.timex.eu/home';
		} else if (countryDom === "IN") {
			location.href = 'https://timexindia.com';
		} else {
			location.href = 'https://www.timex.' + countryDom.toLowerCase();
		}
	}
}

//loop the elements to get the flow and undefined countries when click.
for (let i = 0; i < elements.length; i++) {
    elements[i].addEventListener('click', myCountrySelc, false);
	countryName[i].addEventListener('click', selCountryName, false);
}

//loop the elements to get the set country and put check in the list country.
const countryIndex = function () {
	for (let j = 0; j < countryName.length; j++) {
		if (countryName[j].innerHTML == countryDefault.innerHTML) {
			countryName[j].insertAdjacentHTML('afterend', countryCheckSvg);
		} 	
	}
}

//Change the set country at the footer according to the storefront id. 
let countrySiteID = 'timexus';
let imageValue = document.getElementsByClassName('flow-country-picker-dropdown-trigger');
var domainTld = location.hostname.split(".");
domainTld = domainTld[domainTld.length-1];
var countryFlag = domainTld.toUpperCase();

if (location.hostname.indexOf("www") !== 0 ) {
	switch (countrySiteID) {
		case "timexuk":
			document.querySelector('.flow-country-picker-dropdown-trigger-text').innerHTML = 'United Kingdom';
			imageValue[0].getElementsByTagName('img')[0].src = 'https://flowcdn.io/util/icons/flags-v2/svg/GB.svg';
		break;
		case "timexca":
			document.querySelector('.flow-country-picker-dropdown-trigger-text').innerHTML = 'Canada';
			imageValue[0].getElementsByTagName('img')[0].src = 'https://flowcdn.io/util/icons/flags-v2/svg/CA.svg';
		break;
		case "timexpl":
			document.querySelector('.flow-country-picker-dropdown-trigger-text').innerHTML = 'Poland';
			imageValue[0].getElementsByTagName('img')[0].src = 'https://flowcdn.io/util/icons/flags-v2/svg/PL.svg';
		break;
		default:
			document.querySelector('.flow-country-picker-dropdown-trigger-text').innerHTML = 'EU';
			imageValue[0].getElementsByTagName('img')[0].src = 'https://flowcdn.io/util/icons/flags-v2/svg/EU.svg';
	}
	
	countryIndex();
} else {
	switch (countryFlag) {
		case "CA":
			document.querySelector('.flow-country-picker-dropdown-trigger-text').innerHTML = 'Canada';
		break;
		case "FR":
			document.querySelector('.flow-country-picker-dropdown-trigger-text').innerHTML = 'France';
		break;
		case "DE":
			document.querySelector('.flow-country-picker-dropdown-trigger-text').innerHTML = 'Germany';
		break;
		case "IT":
			document.querySelector('.flow-country-picker-dropdown-trigger-text').innerHTML = 'Italy';
		break;
		case "PL":
			document.querySelector('.flow-country-picker-dropdown-trigger-text').innerHTML = 'Poland';
		break;
		case "ES":
			document.querySelector('.flow-country-picker-dropdown-trigger-text').innerHTML = 'Spain';
		break;
		case "UK":
			document.querySelector('.flow-country-picker-dropdown-trigger-text').innerHTML = 'United Kingdom';
			imageValue[0].getElementsByTagName('img')[0].src = 'https://flowcdn.io/util/icons/flags-v2/svg/GB.svg';
		break;
		default:
			document.querySelector('.flow-country-picker-dropdown-trigger-text').innerHTML = 'EU'
	}		

	if (countrySiteID !== 'timexuk') {
		imageValue[0].getElementsByTagName('img')[0].src = 'https://flowcdn.io/util/icons/flags-v2/svg/'+ countryFlag +'.svg';
	}
	countryIndex();
}
</script>
