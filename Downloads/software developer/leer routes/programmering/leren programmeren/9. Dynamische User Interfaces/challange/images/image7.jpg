  function changeCheck(element){
    if (window.matchMedia('(max-width: 767.9px)').matches){
      let allTheInputs = element.parentElement.parentElement;
      for (let a = 0; a < allTheInputs.length; a++){
        allTheInputs[a].querySelector('.sort-options__option input').removeAttribute('checked')
      }
      element.parentElement.querySelector('input').checked = true;
    }
  }
  function closeFilters(element){
    let closeButton = element.parentElement.parentElement.querySelector('.filter-options__close').click()
  }
  function toggleOverflow(){
    document.querySelector('body').classList.toggle('no-scrolling')
    document.querySelector('html').classList.toggle('no-scrolling')

    if (window.matchMedia('(max-width: 767.9px)').matches){
      try {
      let specifiedElement = document.querySelector(document.querySelector('.modal-is-open .filter-options__inner'))
      document.addEventListener('click', event => {
          const isClickInside = specifiedElement.contains(event.target)

          if (!isClickInside) {
            document.querySelector('body').classList.remove('no-scrolling')
                document.querySelector('html').classList.remove('no-scrolling')
          }
        })
      } catch {
      console.log('modal is not open')
      }
    }

  }
