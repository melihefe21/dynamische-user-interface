          let allDataTotal = document.querySelectorAll('[data-filters-total]')
          let breadcrumbs = doc.querySelector('.filter-breadcrumbs')
          let filtersCount = breadcrumbs.querySelectorAll('.filter-pills').length
        if (doc.querySelector('.filter-breadcrumbs .filter-pills') !== null){
          for (let a = 0; a < allClearButtons.length; a++){
          allClearButtons[a].classList.remove('hidden')
          }
          for (let a = 0; a < allDataTotal.length; a++){
          allDataTotal[a].innerText = "(" + filtersCount + ")"
          }
        } else {
         for (let a = 0; a < allClearButtons.length; a++){
          allClearButtons[a].classList.add('hidden')
          }
        }
      })
    }, 1200)
    
