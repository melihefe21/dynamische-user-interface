function addToCart(){

try {
        let parsedId = parseInt(document.querySelector('.warranty-selector.active').getAttribute('data-variant-id'))
        console.log(parsedId)
        let formData = {
    'items': [{
      'id': parsedId,
      'quantity': 1
      }]
    }

    fetch('/cart/add.js', {
      method: 'POST',
      headers: {
        'Content-Type': 'application/json'
      },
      body: JSON.stringify(formData)
    })
    .then(response => {
      console.log('added to cart')
      window.location.href="/checkout"
    })
    .catch((error) => {
      console.error('Error:', error);
    });
} catch {
  window.location.href="/checkout"
}

}

function removeItemToCart(varId){
    setTimeout(function(){
      document.querySelector(`cart-remove-button[data-id="`+ varId +`"]`).click();
    }, 1000)

}

</script>
    
