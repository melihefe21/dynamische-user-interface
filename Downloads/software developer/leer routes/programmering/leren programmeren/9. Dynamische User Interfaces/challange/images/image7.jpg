<script>
function toggleCollapsible(element){
  element.parentElement.classList.toggle('collapsible-is-open')
}

  function addCollapsibleToggle(){
  setTimeout(function(){
        for (let a = 0; a < document.querySelectorAll('filter-options__details [data-collapsible-trigger]').length; a++){
    document.querySelectorAll('filter-options__details [data-collapsible-trigger]')[a].setAttribute('onclick', 'toggleCollapsible(this)')
      }
  }, 1000)
}
let openedModal;
function toggleModalClass(element, action){
  const overlay = document.querySelector('.filter-options__submenu toggle-element-class.cart-drawer-overlay')
  const openedModals = document.querySelectorAll('.modal-is-open');
  const modals = document.querySelectorAll('.modal-content')
 
  if (action == 'open'){
    element.parentElement.parentElement.parentElement.parentElement.parentElement.classList.add('element-modal-opened')
    if (overlay && openedModals.length < 1) {
      overlay.style.display = 'block'
    }
    modals.forEach(modal => {
      if (element.innerText.toLowerCase().includes('size') && modal.innerText.toLowerCase().includes('size')) {
        openedModal = modal
        return;
      } else if (element.innerText.toLowerCase().includes('more') && modal.innerText.toLowerCase().includes('more') ) {
        openedModal = modal 
        return;
      }
    })

    if (openedModal) {
      openedModal.classList.add('modal-content--open')
    }
  } else if (action == 'close'){
    element.parentElement.parentElement.parentElement.parentElement.parentElement.classList.remove('element-modal-opened')
    if (overlay) overlay.style.display = 'none'
    if (openedModal) {
      openedModal.classList.remove('modal-content--open')
      openedModal = null
    }
  }
}
</script>
