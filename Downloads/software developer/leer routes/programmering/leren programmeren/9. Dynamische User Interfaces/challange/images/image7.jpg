function loadFilter(element){
    setTimeout(function(){
      fetch(window.location.href).then(function(response){
      return response.text()
      }).then(function (html){
        let parser = new DOMParser();
        let doc = parser.parseFromString(html, 'text/html')
        let pills = doc.querySelector('.filter-options__header').innerHTML
        let allHeaders = document.querySelectorAll('.filter-options__header');
          for (let a = 0; a < allHeaders.length; a++){
            allHeaders[a].innerHTML = pills;
          }
          let allClearButtons = document.querySelectorAll('.filter-options__clear-all')
          let allDataTotal = document.querySelectorAll('[data-filters-total]')
          let breadcrumbs = doc.querySelector('.filter-breadcrumbs')
          let filtersCount = breadcrumbs.querySelectorAll('.filter-pills').length
        if (doc.querySelector('.filter-breadcrumbs .filter-pills') !== null){
          for (let a = 0; a < allClearButtons.length; a++){
          allClearButtons[a].classList.remove('hidden')
          }
          for (let a = 0; a < allDataTotal.length; a++){
          allDataTotal[a].innerText = "(" + filtersCount + ")"
          }
        } else {
         for (let a = 0; a < allClearButtons.length; a++){
          allClearButtons[a].classList.add('hidden')
          }
        }
      })
    }, 1200)
    
  };
  function setFilter(element){
    if (element.getAttribute('data-pills').indexOf(':') != -1){
    let pillData = element.getAttribute('data-pills').replace(':', ' ');
    pillData = pillData.replace(/\s/g, '-').toLowerCase()
    pillData = pillData.replace('--', '-')
    pillData = pillData.replace('%', '')
          let parentWrapper = element.parentElement.parentElement.parentElement;
          parentWrapper.querySelector(`[data-filter-option="${pillData}"]`).click();
        } else {
              let pillData = element.getAttribute('data-pills').replace(/\s/g, '-').toLowerCase();
              pillData = pillData.replace('--', '-')
          let parentWrapper = element.parentElement.parentElement.parentElement;
          parentWrapper.querySelector(`[data-filter-option="${pillData}"]`).click();
