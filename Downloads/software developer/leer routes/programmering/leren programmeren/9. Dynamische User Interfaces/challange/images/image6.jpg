		<meta name="theme-color" content=""><script>
            const GTM = {

                /* VISITOR */
                
                    visitorType          : 'not_logged',
                    visitorId            : '',
                    customerEmail        : '',
                    customerPurchasesCout: '',
                    customerPurchasesTot : '',
                

                /* PRODUCT */
                
                    is_product      : false,
                    product_name    : '',      // 'Ultra Thin Bracelet 40 mm - Creepy Chic',
                    product_id      : '',      // 'D1-UTBJ17',
                    product_price   : '',      // '395.00',
                    product_brand   : '',      // 'D1 Milano',
                    product_category: '',      // 'Watch',
                    product_variant : '',      // 'Creepy Chic',
                

                related_impressions: [], // Related products array
                best_sellers_impressions: [],

                /* CURRENCY */
                currencyCode: 'USD',   // 'USD',

                /* FIRST TIME */
                is_first_time: false,

                /* INIT */
                init: function(){
                    document.addEventListener("DOMContentLoaded", function(){
                        GTM.pageView();
                        if (GTM.is_product) GTM.pushPDP();
                    });
                },

                /* OK */
                pageView: function(){
                    dataLayer.push({
                        'visitorType'          : GTM.visitorType,
                        'visitorId'            : GTM.visitorId,
                        'customerEmail'        : GTM.customerEmail,
                        'customerPurchasesCout': GTM.customerPurchasesCout,
                        'customerPurchasesTot' : GTM.customerPurchasesTot
                    });
                },

                /* OK */
                pushPLP: function(impressions){
                    impressions.forEach(function (impression, i) {
                        impressions[i].position = (i + 1);
                        if(impressions[i].variant){
                            impressions[i].variant = impressions[i].variant.trim();
                        }
                        else{
                            impressions[i].variant = '';
                        }
                    });
                    dataLayer.push({
                        'event'                : 'productListView',
                        'visitorType'          : GTM.visitorType,
                        'visitorId'            : GTM.visitorId,
                        'customerEmail'        : GTM.customerEmail,
                        'customerPurchasesCout': GTM.customerPurchasesCout,
                        'customerPurchasesTot' : GTM.customerPurchasesTot,
                        'ecommerce'            : {
                            'currencyCode': GTM.currencyCode,   //'USD',
                            'impressions' : impressions
                        }
                    });
                },

                /* OK */
                pushSearch: function(impressions_in){
                    impressions_out = [];
                    impressions_in.forEach(function (impression_in, i) {
                        let temp_impression          = {};
                            temp_impression.name     = impression_in.title;
                            temp_impression.category = impression_in.type;
                            temp_impression.brand    = impression_in.vendor;
                            temp_impression.position = (i + 1);
                            temp_impression.list     = 'Search_results';
                            temp_impression.id       = impression_in.variants[0].sku;
                            temp_impression.price    = (impression_in.price / 100).toFixed(2);
                        let variant_name             = '';
                        if(impression_in.title.split("-").length > 1){
                            variant_name = impression_in.title.split("-")[1].trim();
                        }
                        temp_impression.variant  = variant_name;
                        impressions_out.push(temp_impression);
                    });
                    dataLayer.push({
                        'event'                : 'productListView',
                        'visitorType'          : GTM.visitorType,
                        'visitorId'            : GTM.visitorId,
                        'customerEmail'        : GTM.customerEmail,
                        'customerPurchasesCout': GTM.customerPurchasesCout,
                        'customerPurchasesTot' : GTM.customerPurchasesTot,
                        'ecommerce'            : {
                            'currencyCode': GTM.currencyCode,   //'USD',
                            'impressions' : impressions_out
                        }
                    });
                },

                /* OK */
                pushRelatad: function(){
                    impressions_out = [];
                    GTM.related_impressions.forEach(function (impression_in, i) {
                        let temp_impression          = {};
                            temp_impression.name     = impression_in.title;
                            temp_impression.category = impression_in.type;
                            temp_impression.brand    = impression_in.vendor;
                            temp_impression.position = (i + 1);
                            temp_impression.list     = 'Related_products';
                            temp_impression.id       = impression_in.variants[0].sku;
                            temp_impression.price    = (impression_in.price / 100).toFixed(2);
                        let variant_name             = '';
                        if(impression_in.title.split("-").length > 1){
                            variant_name = impression_in.title.split("-")[1].trim();
                        }
                        temp_impression.variant  = variant_name;
                        impressions_out.push(temp_impression);
                    });
                    dataLayer.push({
                        'event'                : 'productListView',
                        'visitorType'          : GTM.visitorType,
                        'visitorId'            : GTM.visitorId,
                        'customerEmail'        : GTM.customerEmail,
                        'customerPurchasesCout': GTM.customerPurchasesCout,
                        'customerPurchasesTot' : GTM.customerPurchasesTot,
                        'ecommerce'            : {
                            'currencyCode': GTM.currencyCode,   //'USD',
                            'impressions' : impressions_out
                        }
                    });
                },

                /* OK */
                pushPDP: function(){
                    dataLayer.push({
                        'event'                : 'productDetailView',
                        'visitorType'          : GTM.visitorType,
                        'visitorId'            : GTM.visitorId,
                        'customerEmail'        : GTM.customerEmail,
                        'customerPurchasesCout': GTM.customerPurchasesCout,
                        'customerPurchasesTot' : GTM.customerPurchasesTot,
                        'ecommerce': {
                            'currencyCode': GTM.currencyCode,   //'USD',
                            'detail'      : {
                                'products': [{
                                    'name'    : GTM.product_name,       //'Ultra Thin Bracelet 40 mm - Creepy Chic',
                                    'id'      : GTM.product_id,         //'D1-UTBJ17',
                                    'price'   : GTM.product_price,      //'395.00',
                                    'brand'   : GTM.product_brand,      //'D1 Milano',
                                    'category': GTM.product_category,   //'Watch',
                                    'variant' : GTM.product_variant     //'Creepy Chic',
                                }]
                            }
                        }
                    });
                },

                /* OK */
                cartView: function(productArray){
                    let cartContent = [];
                    productArray.forEach(function (cart_item, i) {
                        p_variant = "";
                        if(cart_item.product_title.split("-").length > 1){
                            p_variant = cart_item.product_title.split("-")[1].trim();
                        }
                        cartContent[i] = {
                            'name'    : cart_item.product_title,              //'Ultra Thin Bracelet 40 mm - Creepy Chic',
                            'id'      : cart_item.sku,                        //'D1-UTBJ17',
                            'price'   : (cart_item.price / 100).toFixed(2),   //'395.00',
                            'brand'   : cart_item.vendor,                     //'D1 Milano',
                            'category': cart_item.product_type,               //'Watch',
                            'variant' : p_variant,                            //'Creepy Chic',
                            'quantity': cart_item.quantity                    // 11
                        };
                    });
                    dataLayer.push({
                        'event'                : 'productCartView',
                        'visitorType'          : GTM.visitorType,
                        'visitorId'            : GTM.visitorId,
                        'customerEmail'        : GTM.customerEmail,
                        'customerPurchasesCout': GTM.customerPurchasesCout,
                        'customerPurchasesTot' : GTM.customerPurchasesTot,
                        'currencyCode'         : GTM.currencyCode,            //'USD',
                        'cartContent'          : cartContent
                    });
                },

                /* OK */
                addToCart: function(p_title, p_sku, p_price, p_brand, p_type, p_variant, quantity){
                    if(!p_variant){
                        p_variant = "";
                    }
                    dataLayer.push({
                        'event'    : 'addToCart',
                        'ecommerce': {
                            'currencyCode': GTM.currencyCode,   //'USD',
                            'add'         : {
                                'products': [{
                                    'name'    : p_title,            //'Ultra Thin Bracelet 40 mm - Creepy Chic',
                                    'id'      : p_sku,              //'D1-UTBJ17',
                                    'price'   : p_price,            //'395.00',
                                    'brand'   : p_brand,            //'D1 Milano',
                                    'category': p_type,             //'Watch',
                                    'variant' : p_variant.trim(),   //'Creepy Chic',
                                    'quantity': quantity            //1
                                }]
                            }
                        }
                    });
                },

                /* TODO */
                removeToCart: function(p_title, p_sku, p_price, p_brand, p_type, p_variant, quantity){
                    if(!p_variant){
                        p_variant = "";
                    }
                    dataLayer.push({
                        'event'    : 'removeFromCart',
                        'ecommerce': {
                            'currencyCode': GTM.currencyCode,   //'USD',
                            'remove'      : {
                                'products': [{
                                    'name'    : p_title,            //'Ultra Thin Bracelet 40 mm - Creepy Chic',
                                    'id'      : p_sku,              //'D1-UTBJ17',
                                    'price'   : p_price,            //'395.00',
                                    'brand'   : p_brand,            //'D1 Milano',
                                    'category': p_type,             //'Watch',
                                    'variant' : p_variant.trim(),   //'Creepy Chic',
                                    'quantity': quantity            //1
                                }]
                            }
                        }
                    });
                },
                /* HOME RECOMMENDED*/
                homeRecommended: function(featured_impressions){
                    impressions_out = [];
                    featured_impressions.forEach(function (impression_in, i) {
                        let temp_impression          = {};
                            temp_impression.name     = impression_in.title;
                            temp_impression.category = impression_in.type;
                            temp_impression.brand    = impression_in.vendor;
                            temp_impression.position = (i + 1);
                            temp_impression.list     = 'home';
                            temp_impression.id       = impression_in.variants[0].sku;
                            temp_impression.price    = (impression_in.price / 100).toFixed(2);
                        let variant_name             = '';
                        if(impression_in.title.split("-").length > 1){
                            variant_name = impression_in.title.split("-")[1].trim();
                        }
                        temp_impression.variant  = variant_name;
                        impressions_out.push(temp_impression);
                    });
                    dataLayer.push({
                        'event'                : 'productListView',
                        'visitorType'          : GTM.visitorType,
                        'visitorId'            : GTM.visitorId,
                        'customerEmail'        : GTM.customerEmail,
                        'customerPurchasesCout': GTM.customerPurchasesCout,
                        'customerPurchasesTot' : GTM.customerPurchasesTot,
                        'ecommerce'            : {
                            'currencyCode': GTM.currencyCode,   //'USD',
                            'impressions' : impressions_out
                        }
                    });
                },
                /* SEARCH BEST SELLERS*/
                pushBestSellers: function(){
                    impressions_out = [];
                    GTM.best_sellers_impressions.forEach(function (impression_in, i) {
                        let temp_impression          = {};
                            temp_impression.name     = impression_in.title;
                            temp_impression.category = impression_in.type;
                            temp_impression.brand    = impression_in.vendor;
                            temp_impression.position = (i + 1);
                            temp_impression.list     = 'best-seller';
                            temp_impression.id       = impression_in.variants[0].sku;
                            temp_impression.price    = (impression_in.price / 100).toFixed(2);
                        let variant_name             = '';
                        if(impression_in.title.split("-").length > 1){
                            variant_name = impression_in.title.split("-")[1].trim();
                        }
                        temp_impression.variant  = variant_name;
                        impressions_out.push(temp_impression);
                    });
                    dataLayer.push({
                        'event'                : 'productListView',
                        'visitorType'          : GTM.visitorType,
                        'visitorId'            : GTM.visitorId,
                        'customerEmail'        : GTM.customerEmail,
                        'customerPurchasesCout': GTM.customerPurchasesCout,
                        'customerPurchasesTot' : GTM.customerPurchasesTot,
                        'ecommerce'            : {
                            'currencyCode': GTM.currencyCode,   //'USD',
                            'impressions' : impressions_out
                        }
                    });
                }
            };
