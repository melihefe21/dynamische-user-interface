    add: function(line, qty){
      let formData = {
          updates:{}
      };
      CartEvents.bundles.forEach(function(pair, i){
        if(pair.watch_line == line){
          formData.updates[parseInt(pair.watch_id)] = qty;
        }
      });
      CartEvents.updateCart(formData);
    },

    remove: function(action, var_id, url, line, quantity){
      let first            = false;  // per fare una sola rimozione
      let n_bundle         = 0;
      let engrave_toRemove = false;  //rimane false se tutti gli orologi di una line hanno engrave
      let package_toRemove = false;  //rimane false se tutti gli orologi di una line hanno box
      let remove           = false;
      let remove_all       = false;  //caso eliminazione di tutti gli orologi in una line
      let formData         = {
          updates:[]
      };

      if(CartEvents.dev)console.log(formData.updates);
      //conto quanti prodotti nello stesso line
      CartEvents.bundles.forEach(function(pair, i){
        if(pair.watch_line == line){
          n_bundle = i+1;
          //controllo se tutti gli engrave hanno un watch
          if(pair.engrave_watch_sku == null){
            engrave_toRemove = true;
          }
        }
      });

      CartEvents.bundles_package.forEach(function(pair, i){
        if(pair.watch_line == line){
          n_bundle = i+1;
          //controllo se tutte le box hanno un watch
          if(pair.box_watch_sku == null){
            package_toRemove = true;
          }
        }
      });

      CartEvents.bundles.forEach(function(pair, i){
        //caso remove x
        //rimozione engrave
        if(action == "remove_engrave"){
          if(pair.watch_line !== null)
            formData.updates[pair.watch_line] = pair.watch_qty;   //quantit rimangono uguali negli altri items
          if(pair.engrave_line !== null)
            formData.updates[pair.engrave_line] = pair.engrave_qty;  //gli engrave degli altri items rimangono a 1

          formData.updates[line] = 0;  //elimino il line item selezionato
          remove = true;
        }
        else{
           //caso rimozione da x watch
          if(quantity == 0 && action!="minus"){
            remove_all = true;
            if(pair.watch_line == line){
              formData.updates[pair.watch_line] = 0;     //metto a 0 le quantit degli watch di quel line item
                              pair.watch_sku   = null;  //metto a null tutti gli watch associati agli engrave di quel line item
            }
            else{
              formData.updates[pair.watch_line] = pair.watch_qty;   //quantit rimangono uguali negli altri items
              if(pair.engrave_line !== null)
                formData.updates[pair.engrave_line] = pair.engrave_qty;  //gli engrave degli altri items rimangono a 1
            }
          }
          else{
            //metto tutti gli watch e gli engrave alle quantit attuali
            if(pair.engrave_id !== null){
              formData.updates[pair.engrave_line] = pair.engrave_qty;
            }
            if(pair.watch_id !== null){
              formData.updates[pair.watch_line] = pair.watch_qty;
            }
            //scelgo quale watch eliminare nel caso -1
            if(pair.watch_line == line){
              //eseguo una volta
              if(!first){
                //metto a null quelli che non hanno engrave come priorit del -
                if(pair.engrave_watch_sku == null){
                  pair.watch_sku = null;
                  first          = true;
                }
                //se  un orologio e un engrave elimino quello con l'engrave
                else if(n_bundle <= 1){
                  pair.watch_sku = null;
                  first          = true;
                }
                //se ho pi di un orologio e pi di un engrave elimino il primo engrave
                else if(!engrave_toRemove){
                  pair.watch_sku = null;
                  first          = true;
                }
              }
            }
          }
        }
      });

      CartEvents.bundles_package.forEach(function(pair, i){
        //caso remove x
        //rimozione engrave
        if(action == "remove_package"){
          if(pair.watch_line !== null)
            formData.updates[pair.watch_line] = pair.watch_qty;   //quantit rimangono uguali negli altri items
          if(pair.box_line !== null)
            formData.updates[pair.box_line] = pair.box_qty;  //gli engrave degli altri items rimangono a 1

          formData.updates[line] = 0;  //elimino il line item selezionato
          remove = true;
        }
        else{
           //caso rimozione da x watch
          if(quantity == 0 && action!="minus"){
            remove_all = true;
            if(pair.watch_line == line){
              formData.updates[pair.watch_line] = 0;     //metto a 0 le quantit degli watch di quel line item
                              pair.watch_sku   = null;  //metto a null tutti gli watch associati agli engrave di quel line item
            }
            else{
              formData.updates[pair.watch_line] = pair.watch_qty;   //quantit rimangono uguali negli altri items
              if(pair.box_line !== null)
                formData.updates[pair.box_line] = pair.box_qty;  //le altre box rimangono alle loro quantit
            }
          }
          else{
            //metto tutti gli watch e gli engrave alle quantit attuali
            if(pair.box_id !== null){
              formData.updates[pair.box_line] = pair.box_qty;
            }
            if(pair.watch_id !== null){
              formData.updates[pair.watch_line] = pair.watch_qty;
            }
            //scelgo quale watch eliminare nel caso -1
            if(pair.watch_line == line){
              //eseguo una volta
              if(!first){
                //metto a null quelli che non hanno engrave come priorit del -
                if(pair.box_watch_sku == null){
                  pair.watch_sku = null;
                  first          = true;
                }
                //se  un orologio e un engrave elimino quello con l'engrave
                else if(n_bundle <= 1){
                  pair.watch_sku = null;
                  first          = true;
                }
                //se ho pi di un orologio e pi di un engrave elimino il primo engrave
                else if(!package_toRemove){
                  pair.watch_sku = null;
                  first          = true;
                }
              }
            }
          }
        }
      });

      CartEvents.bundles.forEach(function (pair, i) {
        if(pair.watch_sku == null){
          //caso rimozione da x
          if(remove_all){
            remove = true;
            if(pair.engrave_id !== null){
                formData.updates[pair.engrave_line] = 0; //metto a 0 le quantit di tutti gli engrave con watch_sku null (rimozione con line item)
            }
          }
          //rimozione da -
          else{
            if(pair.engrave_id !== null){
              formData.updates[pair.engrave_line] = pair.engrave_qty-1; //diminuisco di 1 la quantit di un engrave corrispondente ad un watch eliminato
              remove = true;
            }
            if(pair.watch_id !== null){
              formData.updates[pair.watch_line] = parseInt(quantity)>0 ? parseInt(quantity) : 0;  //levo una quantit ad un watch o la metto a 0
              remove = true;
            }
          }
        }
      });

      CartEvents.bundles_package.forEach(function (pair, i) {
        if(pair.watch_sku == null){
          //caso rimozione da x
          if(remove_all){
            remove = true;
            if(pair.box_id !== null){
                formData.updates[pair.box_line] = 0; //metto a 0 le quantit di tutti gli engrave con watch_sku null (rimozione con line item)
            }
          }
          //rimozione da -
          else{
            if(pair.box_id !== null){
              formData.updates[pair.box_line] = pair.box_qty-1; //metto a 0 un engrave corrispondente ad un watch eliminato
              remove = true;
            }
            if(pair.watch_id !== null){
              formData.updates[pair.watch_line] = parseInt(quantity)>0 ? parseInt(quantity) : 0;  //levo una quantit ad un watch o la metto a 0
              remove = true;
            }
          }
        }
      });

      if(CartEvents.dev)console.log(CartEvents.bundles);
      if(CartEvents.dev)console.log(CartEvents.bundles_package);
      if(CartEvents.dev)console.log(formData);
      if(remove){
        CartEvents.updateCart(formData);
      }
      else{
        if(this.is_cart) CartEvents.refreschCart();
      }
    },

    updateCart: function(obj){
      fetch('/cart/update.js', {
        method:"POST",
        headers: {
            'Content-Type': 'application/json;charset=utf-8'
        },
        body: JSON.stringify(obj)
      }).then(data => {
          CartEvents.raise();
      }).catch(error => {
          console.error("Completed with errors: ", error);
      });
    },

    /* Refresh the Cart Icon */
    refreshIcon: function(){
      let counters = document.querySelectorAll('.counterProductsInCart');
      counters.forEach(function (counter, i) {
        counter.innerHTML = CartEvents.cart.item_count;
        if(CartEvents.cart.item_count > 0){
          counter.classList.remove("emptyCart");
        }
        else{
          counter.classList.add("emptyCart");
        }
      });
    },

    /* Refresh the Cart Icon and the Cart Page */
    refreschCart: function(){
      let engraving_prd = false;
      CartEvents.money_format = window.QC_GLOBALS.shop.moneyFormat;
      //refresh elementi cart e order summary
      document.querySelectorAll(".CartItemsList .row").forEach(function(el){
        el.remove();
      });
      document.querySelectorAll(".Summary .rows .row.subtotal").forEach(function(el){
        el.remove();
      });
      document.querySelectorAll(".CartSpecialPackages .watch_container").forEach(function(el){
        el.remove();
      });
      /* Cart icon */
      let counters = document.querySelectorAll('.counterProductsInCart');
      counters.forEach(function (counter, i) {
        counter.innerHTML = CartEvents.cart.item_count;
      });

      /* Cart elements */
      CartEvents.checkExtra();
      CartEvents.styleExtraItems();
      CartEvents.cart.items.forEach(function (cart_item, i) {
        let natural_index = i + 1;
        if(cart_item.product_type.toLowerCase() == 'watch'){
            CartEvents.buildWatch(cart_item, natural_index);
        }
      });
      CartEvents.cart.items.forEach(function (cart_item, i) {
        let natural_index = i + 1;
        if(cart_item.product_type.toLowerCase() == 'engraving'){
          CartEvents.buildEngrave(cart_item, natural_index);
          engraving_prd = true;
        }
      });
      CartEvents.cart.items.forEach(function (cart_item, i) {
        let natural_index = i + 1;
        if(cart_item.product_type.toLowerCase() == 'package'){
            CartEvents.buildPackage(cart_item, natural_index);
        }
      });
      CartEvents.cart.items.forEach(function (cart_item, i) {
        let natural_index = i + 1;
        if(cart_item.product_type.toLowerCase() == 'watch strap'){
            CartEvents.buildWatch(cart_item, natural_index);
        }
      });
      CartEvents.cart.items.forEach(function (cart_item, i) {
        let natural_index = i + 1;
        if(cart_item.product_type.toLowerCase() != 'watch strap' && cart_item.product_type.toLowerCase() != 'watch' && cart_item.product_type.toLowerCase() != 'engraving' && cart_item.product_type.toLowerCase() != 'package'){
            CartEvents.buildWatch(cart_item, natural_index);
        }
        CartEvents.buildOrderSummary(cart_item, natural_index);
      });
      CartEvents.cart.items.forEach(function (cart_item, i) {
        let natural_index = i + 1;
        if(CartEvents.boxable_watch.length > 0){
          if(cart_item.product_type.toLowerCase() == 'watch' && CartEvents.boxable_watch.includes(cart_item.variant_id)){
            CartEvents.buildWatchInPackage(cart_item, natural_index);
          }
          document.querySelectorAll('.unavailable_msg').forEach(function(element){
            element.classList.add('hidden');
            element.classList.remove('visible');
          });
          document.querySelectorAll('.CartSpecialPackages .packages.row').forEach(function(element){
            element.classList.add('visible');
            element.classList.remove('hidden');
          });
        }
        else{
          document.querySelectorAll('.unavailable_msg').forEach(function(element){
            element.classList.add('visible');
            element.classList.remove('hidden');
          });
          document.querySelectorAll('.CartSpecialPackages .packages.row').forEach(function(element){
            element.classList.add('hidden');
            element.classList.remove('visible');
          });
        }
      });
      //svuoto textarea, levo checkbox e levo messaggi di errori, riabilito text area
      document.querySelectorAll('.package_watches .textarea').forEach(function(element){
        element.value = '';
        element.disabled = false;
      });
      document.querySelectorAll('.package_watches .msg').forEach(function(element){
        element.checked = false;
      });
      document.querySelectorAll('.package_watches .error_msg').forEach(function(element){
        element.classList.remove('visible');
        element.classList.add('hidden');
        element.innerHTML = '';
      });
      //check special package visibility
      CartEvents.checkSpecialPackages();

      //check engrave messages
      let messages = null;
      if(engraving_prd){
        CartEvents.addCartMessage(
          CartEvents.messages.engrave_title,
          CartEvents.messages.engrave_text,
          "engrave"
        );
      }
      else{
        messages = document.querySelectorAll(".AnnouncementBarCart .engrave");
        messages.forEach(function(message){
          message.classList.add("hide");
        });
      }

      // [D1-601]
      let preorder_status = CartEvents.checkPreorder();
      switch (preorder_status) {
        case "preorder":
          // Mostro/Creo messaggio preoder
          CartEvents.addCartMessage(
            CartEvents.messages.preorder_title,
            CartEvents.messages.preorder_text,
            "preorder"
          );
          // Nascondo messaggio mixed
          messages = document.querySelectorAll(".AnnouncementBarCart .preorder_mixed");
          messages.forEach(function(message){
            message.remove();
          });
          break;
        case "mixed":
          // Mostro/Creo messaggio preoder
          CartEvents.addCartMessage(
            CartEvents.messages.preorder_title,
            CartEvents.messages.preorder_text,
            "preorder"
          );
          // Mostro/Creo messaggio mixed
          CartEvents.addCartMessage(
            CartEvents.messages.mixed_preorder_title,
            CartEvents.messages.mixed_preorder_text,
            "preorder_mixed"
          );
          break;
        default:
          // Nascondo messaggio preoder
          // Nascondo messaggio mixed
          messages = document.querySelectorAll(".AnnouncementBarCart .preorder, .AnnouncementBarCart .preorder_mixed");
          messages.forEach(function(message){
            message.remove();
          });
      }

      /* full bar check */
      let bar = document.querySelector(".AnnouncementBarCart");
      messages = document.querySelectorAll(".AnnouncementBarCart > div");
      visible_messages_counter = 0;
      messages.forEach(function(message){
        if(!message.classList.contains("hide")){ visible_messages_counter++; }
      });
      // se non ci sono messaggi senza hide sparisce l'interna barra
      if( visible_messages_counter > 0 ){ bar.classList.remove('hide'); }
      else{ bar.classList.add('hide'); }

      /* Header update */
      let headerCart = document.querySelector('.CartItemsList .header span.notranslate');
      if(headerCart){
        headerCart.innerHTML = "(" + CartEvents.cart.item_count + ")";
      }
