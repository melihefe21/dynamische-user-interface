		getActuallang : function(){
			let url_pieces = window.location.href.split('/');

			if( url_pieces.includes("it") ){
				this.actual_lang = "it";
			}
			else{
				if( url_pieces.includes("ar") ){
					this.actual_lang = "ar";
				}
				else{
					this.actual_lang = "en";
				}
			}
		},

		searchUrlParams: function (){
			let href = new URL(window.location.href);
			if(href.search.length > 0){
				return href.search;
			}
			return '';
		},

		fullUrl: function(baseUrl, additionalPath, language){
			let fullUrl                   = '';
			let url_lang                  = '';
			let last_baseUrl_char         = baseUrl.slice(-1);
			let first_additionalPath_char = additionalPath.substring(0, 1);
				additionalPath            = additionalPath.replace("/it/", "/").replace("/ar/", "/");

			if (additionalPath == "/it" || additionalPath == "/ar") additionalPath = "/";

			if(language != 'EN'){
				if(baseUrl.includes('eu')){
					url_lang = '/' + 'it' + '/';
				}
				else{
					url_lang = '/' + 'ar' + '/';
				}
			}
			else{
				url_lang = '/';
			}

			if(last_baseUrl_char == "/" && first_additionalPath_char == "/"){
				fullUrl = this.trimRightSlashes(baseUrl) + url_lang + this.trimLeftSlashes(additionalPath.slice(1));
			}
			else{
				fullUrl = this.trimRightSlashes(baseUrl) + url_lang + this.trimLeftSlashes(additionalPath);
			}
			return fullUrl;
		},

