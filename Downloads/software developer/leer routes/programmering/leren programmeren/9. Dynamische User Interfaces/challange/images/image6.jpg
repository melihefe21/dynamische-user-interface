        background-color: #fff;
        color           : #000;
    }
    #banner-redirect button:nth-of-type(2),
    #pre-banner-redirect button:nth-of-type(2){
        margin-top: 10px;
    }
    #banner-redirect .informations-container{
        margin-top   : 25px;
        margin-bottom: 18px;
    }
    #banner-redirect .informations-container p{
        font-family  : 'Montserrat';
        font-size    : 9px;
        font-weight  : 500;
        margin-bottom: 5px;
    }
    #banner-redirect .payment-methods p{
        font-family: 'Montserrat';
        font-size  : 10px;
        font-weight: 600;
    }
    #banner-redirect .payment-methods ul{
        display   : flex;
        column-gap: 15px;
        margin    : 10px 0 0 0;
        padding   : 0;
    }
    #banner-redirect .payment-methods ul svg{
        width: 100%;
    }
    @media (max-width: 1280px){
        #banner-redirect{
            padding: 20px 10px 10px 10px;
        }
        #pre-banner-redirect{
            padding: 20px 10px 20px 10px;
        }
        #banner-redirect .modal-title,
        #pre-banner-redirect .modal-title{
            font-size    : 22px;
            margin-bottom: 5px;
        }
        #banner-redirect .banner-redirect__alert-text__content,
        #pre-banner-redirect .banner-redirect__alert-text__content{
            font-size: 14px;
        }
        #banner-redirect .banner-redirect__components,
        #pre-banner-redirect .banner-redirect__components{
            padding-top: 20px;
        }
        #banner-redirect .banner-redirect__select,
        #pre-banner-redirect .banner-redirect__select{
            margin-bottom: 5px;
        }
        #banner-redirect .informations-container{
            margin-top: 15px;
        }
    }
    @media (max-width: 768px){
        #banner-redirect,
        #pre-banner-redirect{
            width  : 300px;
            padding: 30px 15px;
        }
        #banner-redirect .banner-redirect__container,
        #pre-banner-redirect .banner-redirect__container{
            max-width: 197px;
            margin   : 0 auto;
        }
        #banner-redirect #close,
        #pre-banner-redirect .close{
            width : 7px;
            height: 7px;
        }
        #banner-redirect .modal-title,
        #pre-banner-redirect .modal-title{
            font-size     : 18px;
            letter-spacing: 0.9px;
        }
        #banner-redirect .banner-redirect__alert-text__content,
        #pre-banner-redirect .banner-redirect__alert-text__content{
            font-size: 12px;
        }
        #banner-redirect .banner-redirect__components,
        #pre-banner-redirect .banner-redirect__components{
            padding-top: 15px;
            max-width  : 197px;
        }
        #banner-redirect .banner-redirect__select,
        #pre-banner-redirect .banner-redirect__select{
            font-size      : 12px;
            background-size: 0.55em auto, 100%;
            height         : 30px;
        }
        #banner-redirect button,
        #pre-banner-redirect button{
            height     : 30px;
            line-height: 30px;
            font-size  : 12px;
        }
        #banner-redirect .payment-methods ul{
            column-gap: 5px;
        }
    }
</style>

