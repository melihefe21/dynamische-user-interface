    validatePackage: function(target){
      let row_closest   = target.closest('.package_watches');
      let error_msg     = row_closest.querySelector('.error_msg');
      let valid         = true;
      let group         = target.getAttribute('data-group');
      let radio_value   = row_closest.querySelectorAll('input[name="radio_group_'+group+'"]');
      let radio_check   = false;
      let checkboxBlank = false;
      let message       = '';
      radio_value.forEach(function(elem){
        if(elem.checked){
          radio_check = true;
        }
      });
      if(CartEvents.dev) console.log(radio_check);
      if(row_closest.querySelector('input[type="checkbox"]') && row_closest.querySelector('.textarea')){
        checkboxBlank = row_closest.querySelector('input[type="checkbox"]').checked;
        message       = row_closest.querySelector('.textarea').value;
      }
      else{
        checkboxBlank = true;
      }
      if(CartEvents.dev) console.log(checkboxBlank);

      if(radio_check){
        if(checkboxBlank){
          if(message != ''){
            valid               = false;
            error_msg.innerHTML = CartEvents.messages.error_msg_and_checkbox;
            error_msg.classList.add('visible');
          }
          else{
            error_msg.classList.remove('visible');
          }
        }
        else{
          if(message != ''){
            error_msg.classList.remove('visible');
          }
          else{
            valid               = false;
            error_msg.innerHTML = CartEvents.messages.error_nomsg_nocheck;
            error_msg.classList.add('visible');
          }
        }
      }
      else{
        valid = false;
        error_msg.innerHTML = CartEvents.messages.radio_error;
        error_msg.classList.add('visible');
      }
      return valid;
    },

