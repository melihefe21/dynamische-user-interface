  }
</style>

<script>
/* funzione che mi va a scrivere il cookie */
function setCookie(name,value,days) {
    var expires = "";
    if (days) {
        var date = new Date();
        //date.setTime(date.getTime() + (days*24*60*60*1000));
        date.setTime(date.getTime() + (days*24*60*60*1000));
        expires = "; expires=" + date.toUTCString();
    }
    document.cookie = name + "=" + (value || "")  + expires + "; path=/";
}

/* dochiarazione variabili bottoni da usare */
document.addEventListener('DOMContentLoaded', function(){
  var openNewsletter   = document.querySelector('#openNewsletter');
  var closeNewsletter  = document.querySelector("#closeNewsletterBlock");
  var layoutNewsletter = document.querySelector('.newsletter-layout');
  var newsletter       = document.querySelector('.klavio-newsletter');

  openNewsletter.addEventListener('click', function (event) {
    var newsletterPopup = document.querySelector('.klavio-newsletter');
    var animationBlock  = document.querySelector('.klavio-newsletter .klaviyo-form-T6iCmS');
    newsletterPopup.classList.add("opened");
    layoutNewsletter.classList.add("visible");
    animationBlock.classList.add("anim");
  });

  closeNewsletter.addEventListener('click', function (event) {
    var newsletterPopup               = document.querySelector('#newsletter-block');
        newsletterPopup.style.display = 'none';
    layoutNewsletter.classList.remove('visible');
    setCookie("newsletter_close",'1',365); //set "user_email" cookie, expires in 1 day
  });

  layoutNewsletter.addEventListener('click', function(event){
    var newsletterPopup               = document.querySelector('#newsletter-block');
        newsletterPopup.style.display = 'none';
    newsletter.classList.remove('opened');
    layoutNewsletter.classList.remove('visible');
    setCookie("newsletter_close",'1',365); //set "user_email" cookie, expires in 1 day
  });

});
</script>

