    //CREA I DUE ARRAY DI OGGETTI BUNDLES (ENGRAVE-WATCH  BOX-WATCH)
    createBundle: function(){
      /* Check if the engrave and box items are with the watch */
      let skus = [];
      let skus_pack = [];
      /* let prototype_item = {
          watch_sku         = "pippo",
          engrave_watch_sku = "pippo",
          engrave_id        = "12345",
          engrave_line      = "7"
      }*/

      /* add a WATCH item to the bundles array */
      let add_watch = function(p_id, w_line, w_id, w_qty){
          let found = false;
          skus.forEach(
              function(obj){
                  if (obj.engrave_watch_sku == p_id && obj.watch_sku == null && !found){
                      obj.watch_sku = p_id;
                      obj.watch_id = w_id;
                      obj.watch_line = w_line;
                      obj.watch_qty = w_qty;
                      found = true;
                  }
              }
          );
          if(!found){
              let item = {
                  "watch_sku"        : p_id,
                  "watch_id"         : w_id,
                  "watch_line"       : w_line,
                  "watch_qty"        : w_qty,
                  "engrave_watch_sku": null,
                  "engrave_id"       : null,
                  "engrave_line"     : null,
                  "engrave_qty"      : null
              };
              skus.push(item);
          }
      };

      /* add an ENGRAVE item to the bundles array */
      let add_engrave = function(p_id, e_id, e_line, e_qty){
        let found = false;
        skus.forEach(
            function(obj){
                if (obj.watch_sku == p_id && obj.engrave_id == null && !found){
                    obj.engrave_watch_sku = p_id;
                    obj.watch_id          = obj.watch_id;
                    obj.watch_line        = obj.watch_line;
                    obj.watch_qty         = obj.watch_qty;
                    obj.engrave_id        = e_id;
                    obj.engrave_line      = e_line;
                    obj.engrave_qty       = e_qty;
                    found                 = true;
                }
            }
        );
        if(!found){
            let item = {
                "watch_sku"        : null,
                "watch_id"         : null,
                "watch_line"       : null,
                "watch_qty"        : null,
                "engrave_watch_sku": p_id,
                "engrave_id"       : e_id,
                "engrave_line"     : e_line,
                "engrave_qty"      : e_qty
            };
            skus.push(item);
        }
      };

      /* add a WATCH item to the bundles_package array*/
      let add_watch_pack = function(p_id, w_line, w_id, w_qty){
          let found = false;
          skus_pack.forEach(
              function(obj){
                  if (obj.box_watch_sku == p_id && obj.watch_sku == null && !found){
                      obj.watch_sku = p_id;
                      obj.watch_id = w_id;
                      obj.watch_line = w_line;
                      obj.watch_qty = w_qty;
                      found = true;
                  }
              }
          );
          if(!found){
              let item = {
                  "watch_sku"    : p_id,
                  "watch_id"     : w_id,
                  "watch_line"   : w_line,
                  "watch_qty"    : w_qty,
                  "box_watch_sku": null,
                  "box_id"       : null,
                  "box_line"     : null,
                  "box_qty"      : null
              };
              skus_pack.push(item);
          }
      };

      /* add BOX item to the bundles_package array */
      let add_box = function(p_id, b_id, b_line, b_qty){
          let found = false;
          skus_pack.forEach(
              function(obj){
                  if (obj.watch_sku == p_id && obj.box_id == null && !found){
                      obj.box_watch_sku = p_id;
                      obj.watch_id      = obj.watch_id;
                      obj.watch_line    = obj.watch_line;
                      obj.watch_qty     = obj.watch_qty;
                      obj.box_id        = b_id;
                      obj.box_line      = b_line;
                      obj.box_qty       = b_qty;
                      found             = true;
                  }
              }
          );
          if(!found){
              let item = {
                  "watch_sku"    : null,
                  "watch_id"     : null,
                  "watch_line"   : null,
                  "watch_qty"    : null,
                  "box_watch_sku": p_id,
                  "box_id"       : b_id,
                  "box_line"     : b_line,
                  "box_qty"      : b_qty
              };
              skus_pack.push(item);
          }
      };

      //CREAZIONE BUNDLE ENGRAVE-WATCH AND CREAZIONE BUNDLE BOX-WATCH
      CartEvents.cart.items.forEach(function (cart_item, i) {
        /* case engrave */
        if(cart_item.product_id == CartEvents.engraveProduct_id){
            for (let index = 0; index < cart_item.quantity; index++) {
                add_engrave(
                    cart_item.properties["Watch sku"].split("(")[1].split(")")[0],
                    cart_item.variant_id,
                    (i + 1),
                    cart_item.quantity
                );
            }
        }
        /* case box */
        if(cart_item.product_id == CartEvents.boxProduct1_id ||
          cart_item.product_id == CartEvents.boxProduct2_id ||
          cart_item.product_id == CartEvents.boxProduct3_id ||
          cart_item.product_id == CartEvents.boxProduct4_id ||
          cart_item.product_id == CartEvents.boxProduct5_id){
            for (let index = 0; index < cart_item.quantity; index++) {
                add_box(
                    cart_item.properties["Watch sku"].split("(")[1].split(")")[0],
                    cart_item.variant_id,
                    (i + 1),
                    cart_item.quantity
                );
            }
        }
        /* case watch */
        else{
          for (let index = 0; index < cart_item.quantity; index++) {
            add_watch(cart_item.sku,(i+1),cart_item.variant_id, cart_item.quantity);
            if(cart_item.product_type == "Watch"){
              add_watch_pack(cart_item.sku,(i+1),cart_item.variant_id, cart_item.quantity);
            }
          }
        }
      });

      CartEvents.bundles = skus;
      CartEvents.bundles_package = skus_pack;

      CartEvents.boxable_watch = [];
      CartEvents.bundles_package.forEach(function(bundle_package, i){
        if(bundle_package.box_id === null){
          CartEvents.boxable_watch.push(bundle_package.watch_id);
        }
