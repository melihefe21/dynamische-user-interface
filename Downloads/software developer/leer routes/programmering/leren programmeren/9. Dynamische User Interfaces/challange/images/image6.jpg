		listen: function(selectCountry = null, selectCurrency = null, selectLanguage= null){
			let buttonRedirect  = document.querySelector('#banner-redirect .banner-redirect__button');
			let close           = document.querySelector('#banner-redirect #close');
			let preClose        = document.querySelector('#pre-banner-redirect .close');
			let preContinue     = document.querySelector('#pre-banner-redirect .continue');
			let preChange       = document.querySelector('#pre-banner-redirect .change');
			let layout          = document.querySelector('#oscuratore');
			let footerSelectors = document.querySelectorAll("#StoreSelector .storeSelectorOption");
			let footerSelector  = null;
			let url             = new URL(window.location.href);
			document.addEventListener('click', function(e){
				if(e.target.innerHTML == 'EN' || e.target.innerHTML =='IT' || e.target.innerHTML =='AR'){
					D1.redirect.setCookie('languageD1', e.target.innerHTML, 60, D1.redirect.domain);
				}
			});
			footerSelectors.forEach(el =>{
				if(el.id == "https://eu.d1milano.com/"){
					footerSelector = el;
				}
			})
			document.querySelector(selectCountry).addEventListener('change', function(){
				D1.redirect.setOptions(selectCountry, selectCurrency, selectLanguage);
			});
			if(buttonRedirect){
				buttonRedirect.addEventListener('click', function(){
					D1.redirect.getDataRedirect(selectCountry, selectLanguage, selectCurrency);
				})
			}
			if(close){
				close.addEventListener('click', function(){
					D1.redirect.closePopup();
					setCookiesOnClose();
				})
			}
			if(preClose){
				preClose.addEventListener('click', function(){
					if( D1.redirect.dataReady ) {
						D1.redirect.closePrePopup();
						setCookiesOnClose();
					}
				})
			}
			if(preContinue){
				preContinue.addEventListener('click', function(){
					if( D1.redirect.dataReady ) {
						D1.redirect.preContinueAction();
					}
				})
			}
			if(preChange){
				preChange.addEventListener('click', function(){
					D1.redirect.closePrePopup();
					D1.redirect.openPopup();
				})
			}
			if(layout){
				layout.addEventListener('click', function(){
					D1.redirect.closePopup();
					D1.redirect.closePrePopup();
					setCookiesOnClose();
				})
			}
			if(footerSelector){
				footerSelector.addEventListener('click', function(){
					D1.redirect.removeCookie(D1.redirect.redirectCookieName);
					D1.redirect.openPopup();
				})
			}

			function setCookiesOnClose(){
				D1.redirect.setCookie(D1.redirect.redirectCookieName, D1.redirect.currentStoreurl, 60, D1.redirect.domain);
				D1.redirect.setCookie('countryD1', document.querySelector(selectCountry).selectedOptions[0].value, 60, D1.redirect.domain);
				D1.redirect.setCookie('currencyD1', Currency.currentCurrency, 60, D1.redirect.domain);
				D1.redirect.setCookie('languageD1', Shopify.locale.toUpperCase(), 60, D1.redirect.domain);
			}
		},

		removeCookie : function(name){
			if(this.getCookie(name)){
				this.setCookie(name, '', 0, D1.redirect.domain);
			}
		},

		customFetch : async function(url = ''){
			// Default options are marked with *
			const response = await fetch(url, {
				method: 'GET'
			});
			return response.text();
		},

		getDataRedirect: function(selectCountry, selectLanguage, selectCurrency){
			let countrySelected = document.querySelector(selectCountry).selectedOptions[0];
			let language        = document.querySelector(selectLanguage).selectedOptions[0].value;
			let store           = countrySelected.getAttribute('data-store');
			let pathName        = window.location.pathname;
			let url             = 'https://eu.d1milano.com';
			if(store == 'INT'){
				url = 'https://d1milano.com';
			}
			let redirectUrl     = this.fullUrl(url, pathName, language);
			let iso             = countrySelected.getAttribute('data-iso');
			let currency        = document.querySelector(selectCurrency).selectedOptions[0].value;
			let currentCurrency = Currency.currentCurrency;
			if(this.dev)console.log(this.currentStoreurl);
			if(this.dev)console.log(url);
			//setto i cookie nello store attuale dove faccio il redirect
			D1.redirect.setCookie(this.redirectCookieName, url, 60, D1.redirect.domain);
			D1.redirect.setCookie('countryD1', iso, 60, D1.redirect.domain);
			D1.redirect.setCookie('currencyD1', currency, 60, D1.redirect.domain);
			D1.redirect.setCookie('languageD1', language, 60, D1.redirect.domain);

			//effettuo il redirect nel caso cambio store o cambio lingua
			if(url != this.currentStoreurl || language != this.actual_lang || currency != currentCurrency){
				this.performRedirect(redirectUrl,iso, currency, language);
			}
			else{
				this.closePopup();
			}
		},

		//genera lista options country e currencies
		injectOptions : function(obj,selectCountry = null,selectCurrency = null, selectLanguage = null){
			let option = null;

			//set country get by user ip
			this.customFetch('//www.cloudflare.com/cdn-cgi/trace')
			.then(data => {
				let country_code = data.replace(/(\r\n|\n|\r)/gm,"").split('loc=');  // JSON data parsed by `data.json()` call
				country_code = country_code[1].split('tls=')[0];
				if(D1.redirect.dev) console.log("COUNTRY", country_code);
				D1.geolocalization = country_code;
				let targets = document.querySelectorAll(".pre-banner-redirect-iso-country");
				targets.forEach(function(target){
					let translation = D1.redirect.countryName[country_code];
					if(translation == '' || translation.includes("missing")){
						translation = D1.redirect.buildObject()[country_code].name;
					}
					target.innerHTML = translation;
				});
				document.querySelector("body").dispatchEvent(
					new CustomEvent("localized", {
						bubbles: true,
						detail: { country: country_code },
					})
				);
				//build countries options
				for (const i in obj) {
					// popup country options
					option = document.createElement('option');
					option.setAttribute("data-store", obj[i].store);
					option.setAttribute("data-name", obj[i].name);
					option.setAttribute("data-iso", i);
					option.setAttribute("data-iso-currency", obj[i].currency);
					option.value = i;
					option.innerHTML = obj[i].name;
					document.querySelector(selectCountry).appendChild(option);
				}
				document.querySelectorAll(selectCountry + ' option').forEach(function(option){
					if(option.value == country_code){
						option.selected = true;
					}
				});
				//currency options insert after get country
				for (const iterator of this.currencyList) {
					option           = document.createElement('option');
					option.value     = iterator.code;
					option.innerHTML = iterator.name;
					document.querySelector(selectCurrency).appendChild(option);
				}
				this.setOptions(selectCountry, selectCurrency, selectLanguage);
				this.dataReady = true;
			})

		},

		//setta currency e lingua in base alla country selezionata
		setOptions : function(selectCountry = null, selectCurrency = null, selectLanguage = null){
			let selectedCountry = document.querySelector(selectCountry).selectedOptions[0];
			let currency        = selectedCountry.getAttribute('data-iso-currency');
			let store           = selectedCountry.getAttribute('data-store');
			let browserLang     = window.navigator.language.split('-')[1];
			let langOptions     = document.querySelectorAll('.banner-redirect__select.languages option');
			//imposto la currency di default corrispondente alla country
			document.querySelectorAll(selectCurrency + ' option').forEach(function(option){
				if(option.value == currency){
					option.selected = true;
				}
			});

			//language options
			//reset language options
			langOptions.forEach(function(option){
				option.remove();
			});
			//genero options lingua in base allo store della country selezionata
			for (const iterator of this.languageList) {
				option = document.createElement('option');
				if(store == 'INT'){
					if(iterator.code != 'IT'){
						option.value     = iterator.code;
						option.innerHTML = iterator.name;
						document.querySelector(selectLanguage).appendChild(option);
					}
				}
				else{
					if(iterator.code != 'AR'){
						option.value = iterator.code;
						option.innerHTML = iterator.name;
						document.querySelector(selectLanguage).appendChild(option);
					}
				}
			}
			//in base alla lingua del browser imposto la lingua selezionata se no va in inglese
			document.querySelectorAll(selectLanguage + ' option').forEach(function(option){
				if(option.value == 'EN'){
					option.selected = true;
				}
			});
			document.querySelectorAll(selectLanguage + ' option').forEach(function(option){
				if(option.value == browserLang){
					option.selected = true;
				}
			});
		},

		//funzione principale: controllo redirect=true, controllo cookie
		startRedirectFlow: function () {
			let urlVars = this.getUrlVars();
			let url = new URL(window.location.href);
			if(this.dev)console.log(urlVars);
			//caso parametro redirect true nell'url, quindi set dei vari cookies
			if (urlVars["redirect"] == "true") {
				if(this.dev)console.log("redirect true");
				this.param_redirect = true;
				D1.redirect.setCookie(this.redirectCookieName, this.currentStoreurl, 60, D1.redirect.domain);
				if (urlVars["shippingCountry"]) {
					D1.redirect.setCookie("countryD1", urlVars["shippingCountry"], 60, D1.redirect.domain);
				}
				if (urlVars["currency"]) {
					D1.redirect.setCookie("currencyD1", urlVars["currency"], 60, D1.redirect.domain);
				}
				if (urlVars["language"]) {
					D1.redirect.setCookie("languageD1", urlVars["language"], 60, D1.redirect.domain);
				}
				url.searchParams.delete('redirect');
				url.searchParams.delete('shippingCountry');
				url.searchParams.delete('currency');
				url.searchParams.delete('language');
				window.history.pushState({path:url.href},'',url.href);
				return;
			}
			//caso in cui trovo i cookies settati e se gli url non corrispondono effettuo il redirect
			if(this.dev)console.log(this.redirectCookieName);
			if(D1.redirect.getCookie(this.redirectCookieName)){
				let url      = D1.redirect.getCookie(this.redirectCookieName);
				let code     = D1.redirect.getCookie('countryD1');
				let currency = D1.redirect.getCookie('currencyD1');
				let language = D1.redirect.getCookie('languageD1');
				let pathName = window.location.pathname;
				if(this.dev)console.log(url);
				if(this.dev)console.log(this.currentStoreurl);
				if (url && url != this.currentStoreurl && !url.includes("shopifypreview")) {
					if(this.dev)console.log("url to perform: " + url);
					if(this.dev)console.log("performRedirect");
					let redirectUrl = this.fullUrl(url, pathName, language);
					this.performRedirect(redirectUrl, code, currency, language);
				}
				if(url && url == this.currentStoreurl && !url.includes("shopifypreview")){
					if(this.dev)console.log("REDIRECT TO LANG?");
					if(this.dev)console.log(this.actual_lang);
					if(this.dev)console.log("=");
					if(this.dev)console.log(language);
					if(this.actual_lang.toLowerCase() != language.toLowerCase()){
						if(this.dev)console.log("REDIRECT TO LANG!");
						let redirectUrl = this.fullUrl(url, pathName, language);
						this.performRedirect(redirectUrl, code, currency, language);
					}
				}
			}
			//se non trovo i cookies apro il popup (quindi primo accesso al sito)
			else{
				document.addEventListener('DOMContentLoaded', function(){
					//D1.redirect.openPopup();
					D1.redirect.openPrePopup();
				});
			}
		},

		performRedirect: function(url, code, currency, language = null, country = null){
			if(this.dev)console.log("performRedirect: " + url);
			if(this.dev)console.log("performRedirect: " + code);
			if(this.dev)console.log("currency: " + currency);
			if(this.dev)console.log("language: " + language);

			const params = new URLSearchParams(window.location.search);
			params.set("redirect", "true");
			if( code != null && code != "undefined"){
				params.set("shippingCountry", code);
			}
			if( currency != null && currency != "undefined"){
				params.set("currency", currency);
			}
			if( language != null && language != "undefined"){
				params.set("language", language);
			}
			let actual_params = '';
			if( Array.from(params).length >= 1){
				actual_params = "?" + params.toString();
			}

			if(this.dev)console.log("number of params: " + Array.from(params).length );
			if(this.dev)console.log("params: " + params.toString() );
			if(this.dev)console.log("redirect to: " + url + actual_params );
			window.location.replace( url + actual_params );
		},

		//in customize il redirect non deve partire
		isCustomize: function () {
			let isCustomizing = false;
			if (this.shopifyState == 'customize') {
				isCustomizing = true;
			}
			return isCustomizing;
		},

		//leggo parametri dell'url
		getUrlVars: function () {
			let vars = [],
			hash;
			let hashes = window.location.href.slice(window.location.href.indexOf("?") + 1).split("&");
			if(this.dev)console.log(hashes);
			for (let i = 0; i < hashes.length; i++) {
				hash = hashes[i].split("=");
				if(this.dev)console.log("hash: " + hash);
				if(this.dev)console.log("hash[1]: " + hash[1]);
				vars.push(hash[0]);
				vars[hash[0]] = hash[1];
			}
			if(this.dev)console.log(vars);
			return vars;
		},

		//ricostruisco l'oggetto countryList, in modo da utilizzarlo come un array per ogni country
		buildObject: function(){
			let obj = {};
			obj = Object.fromEntries(
				this.countryList.map( key =>
					[key.code, {
						store:    key.store ? key.store : '',
						name:     key.name,
						currency: key.currency ? key.currency : '',
					}]
				)
			)
			return obj;
		},

		getCookie : function(cname) {
			let name          = cname + "=";
			let decodedCookie = decodeURIComponent(document.cookie);
			let ca            = decodedCookie.split(';');
			for(let i = 0; i < ca.length; i++) {
				let c = ca[i];
				while (c.charAt(0) == ' ') {
					c = c.substring(1);
				}
				if (c.indexOf(name) == 0) {
					return c.substring(name.length, c.length);
				}
			}
			return "";
		},

		setCookie: function(cname, cvalue, exdays, domain = '') {
			const d = new Date();
			d.setTime(d.getTime() + (exdays * 24 * 60 * 60 * 1000));
			let expires = "expires=" + d.toUTCString();
			if(domain != ''){ domain = "domain="+domain+";"; }
			document.cookie = cname + "=" + cvalue + ";" + expires + ";" + domain +"path=/";
		},

		trimLeftSlashes: function(str) {
			return str.replace(/^(\/+)/g, "");
		},

		trimRightSlashes: function(str) {
			return str.replace(/(\/)+$/g, "");
		},

		preContinueAction: function(){

			if(D1.redirect.IsRightStore()){
				D1.redirect.closePrePopup();
				setCookiesOnClose();
			}
			else{
				if(D1.geolocalization){
					let language    = "EN";
					let pathName    = window.location.pathname;
					let code        = D1.geolocalization;
					let rightStore  = D1.redirect.obj[code].store;
					let currency    = D1.redirect.obj[code].currency;
					let destinationtUrl = 'https://eu.d1milano.com';
					if(rightStore == 'INT'){
						destinationtUrl = 'https://d1milano.com';
					}

					let redirectUrl = D1.redirect.fullUrl(destinationtUrl, pathName, language);
					D1.redirect.performRedirect(redirectUrl, code, currency);
				}
			}

		},
		IsRightStore: function(){

			let isRightStore = false;

			if(D1.geolocalization){
				let currentStoreUrl = D1.redirect.currentStoreurl;
				let rightStore      = D1.redirect.obj[D1.geolocalization].store;
				let rightStoreUrl   = 'https://eu.d1milano.com';
				if(rightStore == 'INT'){
					rightStoreUrl = 'https://d1milano.com';
				}
				if(currentStoreUrl == rightStoreUrl){
					isRightStore = true;
				}
			}

			return isRightStore;

		}
	};

	if(!D1.redirect.isCustomize())
		D1.redirect.init();
