        var klaviyo_template = "collection";
        var klaviyo_timeout  = null;

        /* funzione che serve per leggere cookie */
        function getCookie(name) {
            var nameEQ = name + "=";
            var ca = document.cookie.split(';');
            for (var i = 0; i < ca.length; i++) {
                var c = ca[i];
                while (c.charAt(0) == ' ') c = c.substring(1, c.length);
                if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length, c.length);
            }
            return null;
        }

        /* VARIABLES */
        var userEmail               = getCookie("newsletter_close");                 // legge cookie vede se esiste o no
        var klavio_newsletter_block = document.querySelector('.klavio-newsletter');  // div blocco newsletter al caricamento pagina (non form app di klavio)

        /* FUNCTIONS */
        /* vado a spostare x dentro div di klavio iniettato */
        function move_close_in_klavio(close_newsletter_input, div_newsletter_klavio) {
            div_newsletter_klavio.parentNode.insertBefore(close_newsletter_input, div_newsletter_klavio);
        }

        /* controllo se  stato cliccata la x di chiusura del cookie */
        function check_klavio(klavio_newsletter_block) {
            if (getCookie('__kla_id') || klaviyo_template == "product" || klaviyo_template == "index") {
                klavio_newsletter_block.style.display = 'none';
            } else {
                klavio_newsletter_block.style.display = 'block';
            }
        }

        /* controllo se klavio ha iniettato form */
        let newsletter_check = new MutationObserver(mutationRecords => {
            var close_newsletter_input = document.querySelector('#newsletter-x');
            var div_newsletter_klavio  = document.querySelector('.klavio-newsletter .klaviyo-form-T6iCmS > div > form');
            var container_newsletter   = document.querySelector('.klavio-newsletter');

            for (const record of mutationRecords) {
                for (const added of record.addedNodes) {
                    if (added.nodeType === Node.ELEMENT_NODE) {
                        /* klavio ha iniettato form */
                        var newsletterPopup = document.querySelector('#newsletter-block');
                        /* controllo se user ha cliccato chiusura newsletter  */
                        if (getCookie('newsletter_close') || klaviyo_template == "product" || klaviyo_template == "index") {
                            newsletterPopup.style.display = 'none';
                        } else {
                            clearTimeout(klaviyo_timeout);
                            klaviyo_timeout = setTimeout(function(){ newsletterPopup.style.display = 'block'; }, 4000);
                            move_close_in_klavio(close_newsletter_input, div_newsletter_klavio);
                        }
                    } else {
                        container_newsletter.style.display = 'none';
                    }
                }
            }

            /* events */
            //quando clicco sulla x dentro la form della newsletter esso si chiude
            close_newsletter_input.addEventListener('click', function(event) {
                /*div_newsletter_klavio.classList.remove("anim"); */
                document.querySelector(".klavio-newsletter").classList.remove("opened");
                document.querySelector(".klavio-newsletter").style.display = 'none';

                var newsletterPopup = document.querySelector('#newsletter-block');
                newsletterPopup.style.display = 'none';
                setCookie("newsletter_close",'1',365); //set "user_email" cookie, expires in 1 day

            });

        });

        newsletter_check.observe(document.querySelector(".klaviyo-form-T6iCmS"), {
            childList: true, // observe direct children
        });

        // al doc ready controllo presenza di cookie se utente ha cliccato su x
        check_klavio(klavio_newsletter_block);
