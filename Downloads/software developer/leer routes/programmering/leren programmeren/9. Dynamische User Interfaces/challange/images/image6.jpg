      });
    },

    /* Ajax AddToCart */
    addToCart: async function(button, id, quantity, properties = null, callback = null){

      /* Es.
      'properties': {
                      'Watch sku': p_title + " (" + p_sku + ")",
                      'Engrave_1': row_1,
                      'Engrave_2': row_2
                  }
      */

      let pruductData = null;
      let gtmData = {
        p_title  : '',
        p_sku    : '',
        p_price  : '',
        p_brand  : '',
        p_type   : '',
        p_variant: '',
        quantity : quantity
      }
      if(button){
        if(button.dataset.qcProductJson){
          pruductData = JSON.parse(button.dataset.qcProductJson);
          gtmData = {
            p_title  : pruductData.title,
            p_sku    : pruductData.variants[0].sku,
            p_price  : (pruductData.price / 100).toFixed(2),
            p_brand  : pruductData.vendor,
            p_type   : pruductData.type,
            p_variant: pruductData.title.split("-")[1],
            quantity : quantity
          }
        }
      }

      if(button){
          CartEvents.disableElement(button);
      }

      let formData = {};
      if(properties != null){
          formData.items = [{
                  'id'        : id,
                  'quantity'  : quantity,
                  'properties': properties
          }];
      }
      else{
          formData.items = [{
                  'id'        : id,
                  'quantity'  : quantity
          }];
      }

      await fetch('/cart/add.js', {
          method:"POST",
          headers: { 'Content-Type': 'application/json;charset=utf-8' },
          body: JSON.stringify(formData)
      })
      .then(response => response.json())
      .then(data => {
          // do something with the result
          // CartEvents.cart = data;
          if(CartEvents.dev)console.log("Completed with result: ", data);
          if(data.status !== undefined){
            if( callback != null ) { callback(data, button); }
            throw new Error(data.description);
          } else {
            let counters = document.querySelectorAll('.counterProductsInCart');
            counters.forEach(function (counter, i) {
              counter.innerHTML = parseInt(counter.innerHTML) + 1;
            });
            // Aggiungere chiamata
            if(typeof GTM !== 'undefined' && GTM != null) {
              if(gtmData.p_title == ''){
                gtmData = {
                  p_title  : data.items[0].product_title,
                  p_sku    : data.items[0].sku,
                  p_price  : (data.items[0].price / 100).toFixed(2),
                  p_brand  : data.items[0].vendor,
                  p_type   : data.items[0].product_type,
                  p_variant: data.items[0].product_title.split("-")[1],
                  quantity : quantity
                }
              }
              GTM.addToCart(gtmData.p_title, gtmData.p_sku, gtmData.p_price, gtmData.p_brand, gtmData.p_type, gtmData.p_variant, gtmData.quantity);
            }

            // Comunicazione con utente di aggiunta con successo
            if(toastr){
              toastr.success(CartEvents.messages.add_success);
            }
          }
      })
      .then(() => {
          if(CartEvents.is_cart == true){
              CartEvents.raise();
          }
          else{
              CartEvents.load_cart(CartEvents.refreshIcon);
          }
          if(button){
              setTimeout( CartEvents.enableElement(button), 1000);
          }
      })
      .catch(error => {
          // if any error occured, then catch it here
          console.error("Completed with errors: ", error);
      })
      .finally(() => {
        if(button){
          CartEvents.enableElement(button);
        }
      });
    },

