  <style>
    body {
      font-family: DINPro, sans-serif;
      font-weight: 400;
      font-size: 16px;
      color: #333;
    }
    h1, h2, h3, p {
        margin: 0;
        padding: 0;
    }
    ul {
        list-style: none;
    }
    .visually-hidden {
        position: absolute !important;
        overflow: hidden;
        clip: rect(0 0 0 0);
        height: 1px;
        width: 1px;
        margin: -1px;
        padding: 0;
        border: 0;
    }
    .skip-link:focus {
        clip: auto;
        width: auto;
        height: auto;
        margin: 0;
        padding: 10px;
        opacity: 1;
        z-index: 10000;
        transition: none;
    }
    input[type="number"]::-webkit-inner-spin-button, input[type="number"]::-webkit-outer-spin-button {
        -webkit-appearance: none;
        margin: 0;
    }
    input {
        -webkit-appearance: none;
    }
    input {
        border-radius: 0;
    }
    button {
        font-family: DINPro, sans-serif;
        -webkit-appearance: none;
        border-radius: 0;
    }
    .wrapper {
        width: 1800px;
        margin: 0 auto;
        position: relative;
        height: 100%;
    }
    @media (max-width: 1900px) {
        .wrapper {
            width: 100%;
      }
    }
    .wrapper.small {
        width: 1200px;
    }
    @media (max-width: 1900px) {
        .wrapper.small {
            width: 1200px;
      }
    }
    @media (max-width: 1240px) {
        .wrapper.small {
            width: 100%;
            padding-left: 20px;
            padding-right: 20px;
            box-sizing: border-box;
      }
    }
    .ProductCard {
        width: 100%;
        outline: none;
    }
    .ProductCard .productLink {
        text-decoration: none;
        position: relative;
        display: block;
        margin: 10px 10px 0px 10px;
        outline: none;
        box-sizing: border-box;
        margin-top: 30px;
        padding-bottom: 20px;
        -moz-transition: 0.3s all;
        -o-transition: 0.3s all;
        -webkit-transition: 0.3s all;
        transition: 0.3s all;
    }
    .ProductCard .productLink .label {
        position: absolute;
        background-color: #333;
        color: #fff;
        font-size: 13px;
        left: 0;
        top: 0;
        font-weight: 500;
        text-transform: uppercase;
        padding: 8px 12px 6px 12px;
        letter-spacing: 0.04em;
        z-index: 1;
        border-radius: 2px;
    }
    .ProductCard .productLink .engrave_icon{
        position: absolute;
        top: 0;
        right: 0; /* right: 10px; */
        width: 40px;
    }
    @media(max-width: 960px){
        .ProductCard .productLink .engrave_icon{
            width: 30px;
        }
    }
    .ProductCard .productLink .label.soldout{
        font-family: 'Montserrat';
        font-weight: 700;
        font-size: 16px;
    }
    @media(max-width: 960px){
        .ProductCard .productLink .label.soldout{
            font-size: 10px;
        }
    }
    .ProductCard .productLink .productImageContainer {
        padding: 20% 0;
        position: relative;
        background-color: #f3f3f3;
        border-radius: 2px;
    }
    .ProductCard .productLink .productImageContainer .productImage {
        width: 100%;
        display: block;
        opacity: 1;
    }
    .ProductCard .productLink .productImageContainer .productImageHover {
        position: absolute;
        width: 100%;
        display: block;
        top: 0;
        left: 0;
        opacity: 0;
        transform: translateY(-50%);
        top: 50%;
    }
    @media (min-width: 1050px){
        .ProductCard .productLink .productImageContainer:hover .productImage {
            opacity: 0;
      }
        .ProductCard .productLink .productImageContainer:hover .productImageHover {
            opacity: 1;
      }
    }
    .ProductCard .productLink .productTitle {
        text-align: center;
        color: #333;
        letter-spacing: 0.1em;
        font-weight: 500;
        text-transform: uppercase;
        text-decoration: none;
        font-size: 16px;
        padding: 0;
        padding-bottom: 10px;
        padding-top: 20px;
    }
    .ProductCard .productLink .productCollection {
        text-align: center;
        color: #999;
        text-decoration: none;
        letter-spacing: 0.1em;
        font-size: 13px;
        font-weight: 400;
        padding-bottom: 10px;
        text-transform: uppercase;
    }
    .ProductCard .discountNote {
        width: 100%;
        font-size: 14px;
        font-weight: 400;
        text-align: center;
        padding-top: 15px;
    }
    .ProductCard .discountNote .discountPrice {
        font-size: 16px;
        font-weight: 500;
        padding-right: 5px;
    }
    .ProductCard .productPrice {
        letter-spacing: 0.06em;
        font-size: 14px;
        font-weight: 400;
        color: #333;
        text-align: center;
    }
    .ProductCard .productPrice span {
        display: block;
    }
    .ProductCard .productPrice.onSale .compareAtPrice {
        font-size: 12px;
        text-decoration: line-through;
        padding-bottom: 2px;
        display: inline-block;
    }
    .ProductCard .productPrice.onSale .price {
        font-weight: 500;
        display: inline-block;
    }
    .ProductCard .ProductAddToCartButton {
        text-align: center;
        margin: 0 10px;
    }
    .ProductCard .ProductAddToCartButton button {
        width: 100%;
        height: 40px;
        display: inline-block;
        text-align: center;
        letter-spacing: 0.1em;
        font-size: 14px;
        font-weight: 500;
        text-transform: uppercase;
        text-decoration: none;
        background-color: #fff;
        border: 1px solid #000;
        -moz-transition: all 0.3s;
        -o-transition: all 0.3s;
        -webkit-transition: all 0.3s;
        transition: all 0.3s;
        border-radius: 2px;
    }
    .ProductCard .ProductAddToCartButton button:hover {
        background-color: #000;
        cursor: pointer;
        color: #fff;
    }
    .ProductCard .ProductAddToCartButton button:disabled {
        background-color: #b1b1b1;
        border: 1px solid #b1b1b1;
        color: #f0f0f0;
    }
    .ProductCard .ProductAddToCartButton button:disabled:hover {
        background-color: #b1b1b1;
        color: #f0f0f0;
        cursor: auto;
    }
    .ProductCard .ProductAddToCartButton button .addToCartLabel, .ProductCard .ProductAddToCartButton button .soldOutLabel {
        padding-top: 2px;
        pointer-events: none;
    }
    .ArticleCard .container {
        display: flex;
        height: 60vh;
        flex-direction: row;
        text-decoration: none;
    }
    @media (max-width: 980px) {
        .ArticleCard .container {
            height: 480px;
      }
    }
    @media (max-width: 800px) {
        .ArticleCard .container {
            flex-direction: column;
            height: auto;
      }
    }
    .ArticleCard .container .image {
        height: 100%;
        position: relative;
        background-size: cover;
        background-position: center;
        display: block;
        width: 60%;
        flex: 0 0 auto;
    }
    .ArticleCard .container .content {
        padding: 0 5%;
        width: 40%;
        display: flex;
        box-sizing: border-box;
        flex-direction: column;
        justify-content: center;
        flex: 0 0 auto;
        text-decoration: none;
        color: #333;
    }
    @media (max-width: 1200px) {
        .ArticleCard .container .image {
            width: 50%;
      }
        .ArticleCard .container .content {
            padding: 0 8%;
            width: 50%;
      }
    }
    @media (max-width: 800px) {
        .ArticleCard .container .image {
            width: 100%;
            height: 400px;
      }
        .ArticleCard .container .content {
            width: 100%;
            padding: 10%;
      }
