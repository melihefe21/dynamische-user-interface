          if (event.target.matches("body .quantitySelector button.modifier[data-action='plus']")) {
              event.preventDefault();
               // GTM get data
               CartEvents.temp_item = null;
              if(event.target.closest(".row")){
                CartEvents.temp_item = JSON.parse(event.target.closest(".row").dataset.itemJson);
              }
              // GTM PUSH
              if(typeof GTM !== 'undefined' && GTM != null) {
                let gtmData = {
                  p_title  : CartEvents.temp_item.product_title,
                  p_sku    : CartEvents.temp_item.sku,
                  p_price  : (CartEvents.temp_item.price / 100).toFixed(2),
                  p_brand  : CartEvents.temp_item.vendor,
                  p_type   : CartEvents.temp_item.product_type,
                  p_variant: CartEvents.temp_item.product_title.split("-")[1],
                  quantity : 1
                }
                GTM.addToCart(gtmData.p_title, gtmData.p_sku, gtmData.p_price, gtmData.p_brand, gtmData.p_type, gtmData.p_variant, gtmData.quantity);
              }
              if(event.target.closest(".row")){
                CartEvents.temp_item = JSON.parse(event.target.closest(".row").dataset.itemJson);
              }
              if(CartEvents.dev)console.log('plus');
              CartEvents.add(event.target.getAttribute('data-line'), event.target.getAttribute('data-qty'));
          }
          if(event.target.matches('button.addToCart:not(.fastClick) *, div.AddToCart:not(.fastClick) *')) {
              event.preventDefault();
              if(CartEvents.dev)console.log("Click", event.target);
              let div       = event.target.closest("div.addToCart");
              let button    = event.target.closest("button.addToCart");
              let variantID = button ? button.getAttribute("data-qc-variant-id") : div.getAttribute("data-qc-variant-id");
              let quantity  = button ? button.getAttribute("data-qc-quantity") : div.getAttribute("data-qc-quantity");
              CartEvents.addToCart(button, variantID, quantity);
          }
          //PACKAGE LISTENERS
          if(event.target.matches('.CartSpecialPackages .icon, .CartSpecialPackages .btn_open, .CartSpecialPackages .btn_open *')){
            CartEvents.viewPackage(event.target);
          }
          if(event.target.matches('.CartSpecialPackages .watch_container .check_watch')){
            CartEvents.checkWatch(event.target);
          }
          if(event.target.matches('.CartSpecialPackages .addPackage')){
            event.preventDefault();
            let button        = event.target.closest('button.addPackage');
            let variantID     = button.getAttribute("data-variant-id");
            let sku           = button.getAttribute('data-sku_watch');
            let title         = button.getAttribute('data-watch_title');
            let msg_container = button.closest('.package_watches');
            let msg           = '';
            if( msg_container.querySelector('.textarea')){
              msg = msg_container.querySelector('.textarea').value;
            }
            let props     = {'Watch sku': title + " (" + sku + ")",'Message': msg};
            let validate  = CartEvents.validatePackage(button);
            if(validate){
              window.scrollTo(0,0);
              CartEvents.addToCart(button, variantID, 1, props, CartEvents.addPackageErrorHandler);
            }
            else{
              if(CartEvents.dev)console.log("non valido");
            }
          }
          if(event.target.matches('.CartSpecialPackages input[type="checkbox"]')){
            let closest_row = event.target.closest('.package_watches');
            let element     = closest_row.querySelector(".textarea");
            if(event.target.checked){
              element.value    = "";
              element.disabled = true;
            }
            else{
              element.disabled = false;
            }
          }
          if(event.target.matches('.CartExtraitems .btn_container, .CartExtraItems .btn_container *, .CartExtraitems .btn_container, .CartExtraItems .btn_container')){
            if(CartEvents.dev)console.log("Click Extra add: ", event.target);
            let button = event.target.closest('span');
            if(CartEvents.dev)console.log("Button: ", button);
            if(button === null){
              button = event.target.querySelector('span');
              if(CartEvents.dev)console.log("Button: ", button);
            }
            let variantID = button.getAttribute("data-variant-id");
            if(CartEvents.dev)console.log("variantID: ", variantID);
            let container = event.target.closest('.row');
            if(CartEvents.dev)console.log("container: ", container);
            container.classList.remove('visible');
            container.classList.add('hidden');
            window.scrollTo(0,0);
            CartEvents.addToCart(button, variantID, 1);
          }
      });
    },

    addPackageErrorHandler: function(data, button){
      switch (data.status) {
        case 422:
          let closest_container = button.closest('.package_watches');
          closest_container.querySelectorAll('.error_msg').forEach(function(element){
            element.innerHTML = data.description;
            element.classList.remove('hidden');
            element.classList.add('visible');
          });
          break;
        default:
          if(CartEvents.dev)console.log(data.status);
          break;
      }
      document.querySelectorAll('.package_watches button').forEach(function(element){
        element.disabled = false;
      });
    },

