    Currency.convertTo = function(currencyCode){
        Currency.convertAll(Currency.currentCurrency, currencyCode);
        Currency.currentCurrency = currencyCode;
    };

    Currency.buildSelectors = function(){
        let desktopInsertPoint  = document.querySelector("#currencySelector");
        let mobileInsertPoint   = document.querySelector("#mobileCurrencySelector");
        let currentCurrency_obj = currencies.find(currency => currency.code === Currency.currentCurrency);

        if(desktopInsertPoint !== null){
            let item = '';
            item += currentCurrency_obj.symbol + ' (' + currentCurrency_obj.code + ')';
            item += '<ul class="Selector CurrencySelector">';
            currencies.forEach(currency_obj => {
                if(currency_obj.code === Currency.currentCurrency){
                    item += '<li class="Option selected" onclick="Currency.selectorChange(event);"  data-curr="' + currency_obj.code + '">' + currency_obj.symbol + ' (' + currency_obj.code + ')</li>';
                }
                else{
                    item += '<li class="Option" onclick="Currency.selectorChange(event);" data-curr="' + currency_obj.code + '">' + currency_obj.symbol + ' (' + currency_obj.code + ')</li>';
                }
            });
            item += '</ul>';
            desktopInsertPoint.innerHTML = item;
        }
        if(mobileInsertPoint !== null){
            let item   = '';
            let titolo = '';
            let lista  = '';
            item += '<div class="navigationLink">';
                item += '<span>Currency: </span>';
                item += '<span class="notranslate">' + currentCurrency_obj.symbol + ' (' + currentCurrency_obj.code + ')</span>';
                titolo = currentCurrency_obj.symbol + ' (' + currentCurrency_obj.code + ')';
            item += '</div>';
            item += '<div class="subNavigationList">';
                item += '<button class="back">' + window.localized.layout.navigation.back + '</button>';
                item += '<ul class="Selector CurrencySelector">';
                currencies.forEach(currency_obj => {
                    if(currency_obj.code === Currency.currentCurrency){
                        item  += '<li class="Option selected" onclick="Currency.selectorChange(event);" data-curr="' + currency_obj.code + '"><span class="drawerCloseClass subNavigationLink notranslate">' + currency_obj.symbol + ' (' + currency_obj.code + ')</span></li>';
                        lista += '<li class="Option selected" onclick="Currency.selectorChange(event);" data-curr="' + currency_obj.code + '"><span class="drawerCloseClass subNavigationLink notranslate">' + currency_obj.symbol + ' (' + currency_obj.code + ')</span></li>';
                    }
                    else{
                        item  += '<li class="Option" onclick="Currency.selectorChange(event);" data-curr="' + currency_obj.code + '"><span class="drawerCloseClass subNavigationLink notranslate">' + currency_obj.symbol + ' (' + currency_obj.code + ')</span></li>';
                        lista += '<li class="Option" onclick="Currency.selectorChange(event);" data-curr="' + currency_obj.code + '"><span class="drawerCloseClass subNavigationLink notranslate">' + currency_obj.symbol + ' (' + currency_obj.code + ')</span></li>';
                    }
                });
                item += '</ul>';
            item += '</div>';
            mobileTitle = mobileInsertPoint.querySelector("div.navigationLink .notranslate");
            mobileList  = mobileInsertPoint.querySelector("div.navigationLink .notranslate");
            if(mobileTitle && mobileList){
                mobileInsertPoint.querySelector("div.navigationLink .notranslate").innerHTML  = titolo;
                mobileInsertPoint.querySelector("ul.CurrencySelector").innerHTML = lista;
                Currency.closeMobileCurrencyMenu();
            }
            else{
                mobileInsertPoint.innerHTML = item;
            }
        }
    };

    Currency.selectorChange = function(event){
        CurrTo = event.target.closest(".Option").dataset.curr;
        Currency.convertTo(CurrTo);
        Currency.buildSelectors();
        D1.redirect.setCookie('currencyD1',Currency.currentCurrency,60, D1.redirect.domain);
    };

    Currency.closeMobileCurrencyMenu = function(){
        let drawer = document.querySelector(".Drawer");
        let mobileInsertPoint   = drawer.querySelector(".MobileCustomerNavigation");
        if(mobileInsertPoint !== null){
            mobileInsertPoint.querySelector(".currencySelectorContainer .subNavigationList").classList.remove("visible");
            mobileInsertPoint.querySelector(".currencySelectorContainer").classList.remove("opened");
            drawer.classList.remove("visible");
        }
    };

    Currency.init = function(){Currency.format = 'money_format';Currency.shopCurrency = 'USD';

        /* Sometimes merchants change their shop currency, let's tell our JavaScript file */
        Currency.moneyFormats[Currency.shopCurrency].money_with_currency_format = "${{amount}} USD";
        Currency.moneyFormats[Currency.shopCurrency].money_format               = "${{amount}}";

        // let url_obj = new URL(window.location.href);
        // if( url_obj.searchParams.has('redirect') &&  url_obj.searchParams.get('redirect') == "true"){
        //     console.log(url_obj.searchParams.get('currency'));
        //     Currency.cookie.write( url_obj.searchParams.get('currency'));
        // }
        //console.log(D1.redirect.param_redirect);
        if(D1.redirect.param_redirect){
            //console.log(D1.redirect.getCookie('currencyD1'));
            Currency.cookie.write(D1.redirect.getCookie('currencyD1'));
        }

        /* Default currency */
        Currency.defaultCurrency = 'USD' || shopCurrency;
        Currency.cookieCurrency = Currency.cookie.read();

        if (Currency.cookieCurrency === null) {
            console.log('caso 1');
            if (Currency.shopCurrency !== Currency.defaultCurrency) {
                console.log('caso 1-2');
                Currency.currentCurrency = Currency.defaultCurrency;
                $(function() {
                    Currency.convertAll(Currency.shopCurrency, Currency.defaultCurrency);
                });
            } else {
                console.log('caso 1-3');
                Currency.currentCurrency = Currency.defaultCurrency;
            }
            Currency.buildSelectors();
        } else if (!Currency.isCookieCurrencyAvailable(Currency.cookieCurrency)) {
            //console.log('caso 2');
            console.log(Currency.cookieCurrency);
            // If the cookie value does not correspond to any value in the currency dropdown.
            Currency.currentCurrency = Currency.shopCurrency;
            Currency.cookie.write(Currency.shopCurrency);
            Currency.buildSelectors();
        } else if (Currency.cookieCurrency === Currency.shopCurrency) {
            //console.log('caso 3');
            Currency.currentCurrency = Currency.shopCurrency;
            Currency.buildSelectors();
        } else {
            //console.log('caso 4');
            Currency.currentCurrency = Currency.shopCurrency;
            $(function() {
                Currency.convertAll(Currency.currentCurrency, Currency.cookieCurrency);
                Currency.buildSelectors();
            });
        }

    };
</script>

